#include<stdio.h>
int main()
{ int a[30][30],n,m,i,j,b[30][30];
 printf("Enter the number of rows and columns in an array");
 scanf("%d%d", &n, &m);
 printf("Enter elements for %d x %d arrays", n, m);
 for(i=0;i<n;i++)
 { for(j=0;j<m;j++)
   { scanf("%d", &a[i][j]);
   }
 }

for(i=0;i<n;i++)
{ for(j=0;j<m;j++)
{ printf("%d\t", a[i][j]);
}
 printf("\n");
}
printf("enter another set of elements for another array");
 for(i=0;i<n;i++)
 { for(j=0;j<m;j++)
   { scanf("%d", &b[i][j]);
   }
 }

for(i=0;i<n;i++)
{ for(j=0;j<m;j++)
{ printf("%d\t", b[i][j]);
}
 printf("\n");
}
return 0;
}
