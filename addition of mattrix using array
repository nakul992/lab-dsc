#include<stdio.h>
int main()
{ int a[30][30],b[30][30],c[30][30],i,j,n,m;
printf("enter the rows and colums of the array ");
scanf("%d%d", &n, &m);
printf("Enter the elements for the 1st %d x %d array ", n, m);
for(i=0;i<n;i++)
{for(j=0;j<m;j++)
 {scanf("%d", &a[i][j]);
 }
}
printf("Enter the elements for the 2nd %d x %d array ", n, m);
for(i=0;i<n;i++)
{for(j=0;j<m;j++)
 {scanf("%d", &b[i][j]);
 }
}
for(i=0;i<n;i++)
{for(j=0;j<m;j++)
 {c[i][j]=a[i][j]+b[i][j];
 }
}
for(i=0;i<n;i++)
{for(j=0;j<m;j++)
 {printf("%d\t", c[i][j]);
 }
 printf("\n");
}
return 0;
}
